<div class="content">
  <header>
    <table>
      <tr>
        <td>
          <h1>メモアプリ</h1>
        </td>
        <td id="addBtn">
          <form method="get" action="/memos/new">
            <input id="newBtn" type="submit" value="追加">
          </form>
        </td>
      </tr>
    </table>
  </header>
  <ul>
    <% @memos.each do |memo| %>
    <li>
      <a href="/memos/<%= memo['id'] %>"> <%= memo['title'] %> </a>
    </li>
    <% end %>
  </ul>
</div>
